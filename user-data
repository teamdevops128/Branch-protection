response=$(curl -s -H "Authorization: token $TOKEN" "$GITHUB_URL/orgs/$ORG_NAME/members")

# Check if the request was successful
if [ $? -eq 0 ]; then
    # Parse JSON response to extract user details
    user_details=$(echo "$response" | jq -r '.[] | {login: .login, name: .name, email: .email}')

    # Display user details
    echo "User details for Organization: $ORG_NAME"
    echo "$user_details"
else
    echo "Failed to fetch user details. Check your settings and try again."
fi
